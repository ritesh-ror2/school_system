<div id="subject_list" class="span6 move_down">
  <div>
    <%= link_to_function "Back", "history.back()", :class => "btn btn-primary" %>
  </div>
  <h5><%= t('school_administration.show_parent.record_of_the_parent') %></h5>
  <table class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        <th><%= t('school_administration.show_parent.name') %></th>
        <th><%= t('school_administration.show_parent.email') %></th>
        <th><%= t('school_administration.show_parent.phoneprefix') %></th>
        <th><%= t('school_administration.show_parent.phone') %></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @parent.name%></td>
        <td><%= @parent.email%></td>
        <td><%= @parent.phoneprefix%></td>
        <td><%= @parent.email%></td>
      </tr>
    </tbody>
  </table>
</div>
