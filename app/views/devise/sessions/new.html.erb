<div class="row">
  <div class="login-main-box">
    <h2>Log in to your account</h2>
    <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
      <div class="row">
        <%= f.email_field :email, placeholder:"User name or Email",class: "col-md-12 col-sm-12 col-xs-12 top-space" %>
      </div>  
      <div class="row">
        <%= f.password_field :password, placeholder:"Password", class:"col-md-12 col-sm-12 col-xs-12" %>
      </div>
      <div class="row forgot-pass">
        <div class="col-md-8 col-sm-8 col-xs-12 padd_none">
         <!-- <% if devise_mapping.rememberable? %>
            <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
          <% end %> -->
          <%= render "devise/shared/links" %>
        </div>
        <div class="col-md-4 col-sm-4 col-xs-12 padd_none">
            <%= f.submit "Log in",class:"login" %>
        </div>
      </div>
    <% end %>
    <div class="row click-here">
      is your first access ? <%= link_to 'Click here',signup_users_path %>
    </div>
  </div>  
</div>
